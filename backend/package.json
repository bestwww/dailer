{
  "name": "dialer-backend",
  "version": "1.0.0",
  "description": "Backend API для системы автодозвона с интеграцией FreeSWITCH и Битрикс24",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "tsx watch src/app.ts",
    "dev:debug": "tsx watch --inspect src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "docker:build": "docker build -t dialer-backend .",
    "migrate": "tsx src/scripts/migrate.ts",
    "test-asterisk": "tsx src/scripts/test-asterisk.ts",
    "test-sip-trunk": "tsx src/scripts/test-sip-trunk.ts",
    "migrate:reset": "tsx src/scripts/reset-db.ts",
    "init-auth": "tsx src/scripts/init-auth.ts",
    "check-permissions": "tsx src/scripts/check-permissions.ts",
    "db:check": "tsx src/scripts/check-db.ts",
    "seed": "node dist/config/database.js seed"
  },
  "keywords": [
    "dialer",
    "autodialer",
    "freeswitch",
    "asterisk",
    "voip",
    "typescript",
    "nodejs",
    "bitrix24",
    "telephony"
  ],
  "author": "Dialer System",
  "license": "MIT",
  "dependencies": {
    "@bitrix24/b24jssdk": "^0.4.5",
    "@types/node-cron": "^3.0.11",
    "asterisk-manager": "^0.1.16",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.1.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "modesl": "^1.1.8",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^3.15.9",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-slow-down": "^1.3.5",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/dialer-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/dialer-system/issues"
  },
  "homepage": "https://github.com/your-repo/dialer-system#readme"
}
